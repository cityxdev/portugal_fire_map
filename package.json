{
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.2.0",
    "is-mobile": "^3.1.1",
    "lz-string": "^1.4.4",
    "ol": "^7.1.0",
    "plotly.js-dist-min": "^2.14.0",
    "proj4": "^2.8.0"
  },
  "devDependencies": {
    "parcel": "^2.7.0",
    "parcel-bundler": "^1.12.5"
  },
  "name": "portugal-fire-map",
  "version": "1.0.0",
  "targets": {
    "dashboard": {
      "source": "/index.html",
      "context": "browser",
      "distDir": "./dist"
    },
    "webmap": {
      "source": "/map/index.html",
      "context": "browser",
      "distDir": "./dist/map"
    },
    "about": {
      "source": "/about/index.html",
      "context": "browser",
      "distDir": "./dist/about"
    }
  },
  "scripts": {
    "start": "parcel --target about --target dashboard --target webmap",
    "copy": "mkdir -p dist/config ; cp -R config/* dist/config ; mkdir -p dist/img ; cp -R img/* dist/img "
  },
  "keywords": [],
  "author": "",
  "license": "Apache 2.0",
  "description": ""
}
